package problemsTests;

import static org.junit.Assert.*;
import org.junit.Test;
import problems.ProjectEuler;

public class ProjectEulerTests {
	
	private ProjectEuler projEu = new ProjectEuler();
	
	@Test
	public void sumMultiplesOfThreeAndFive_Input10_Returns23() {
		int expected = 23;
		int actual = projEu.sumMultiplesOfThreeAndFive(10);
		assertEquals(expected, actual);
	}
	
	@Test
	public void sumOfEvenFibNumbers_Input10_Returns10() {
		int expected = 10;
		int actual = projEu.sumOfEvenFibNumbers(10);
		assertEquals(expected, actual);
	}
	
	@Test
	public void findLargestPrimeFactor_Input13195_Returns29(){
		int expected = 29;
		int actual = projEu.findLargestPrimeFactor(13195);
		assertEquals(expected, actual);
	}
	
	@Test
	public void findLargesPalindromeProduct_Input2_Returns9009(){
		int expected = 9009;
		int actual = projEu.findLargestPalindromeProduct(99);
		assertEquals(expected, actual);
	}
	
	@Test
	public void findLowestPositiveNumberEvenlyDivisible_Input10_Returns2520(){
		int expected = 2520;
		int actual = projEu.leastCommonMultiple(10);
		assertEquals(expected, actual);
	}
	
	@Test
	public void sumSquareDifference_Input10_Returns2640(){
		int expected = 2640;
		int actual = projEu.sumSquareDifference(10);
		assertEquals(expected, actual);
	}
	
	@Test public void nThPrime_Input6_Output13(){
		int expected = 13;
		int actual = projEu.nThPrime(6);
		assertEquals(expected, actual);
	}
	

}
