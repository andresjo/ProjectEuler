package problems;

public class ProblemsMainClass {

	public static void main(String[] args) {
		ProjectEuler projEu = new ProjectEuler();		
		long startTime = System.nanoTime();
		
		System.out.println("Sum of multiples of 3 and 5 from 0 to 1000: " 
				+ projEu.sumMultiplesOfThreeAndFive(1000));
		
		System.out.println("Sum of all even Fibonacci-numbers whose "
				+ "value does not exceed four millions: " + projEu.sumOfEvenFibNumbers(4000000));
		
		System.out.println("Largest prime factor of 600851475143 is: " 
				+ projEu.findLargestPrimeFactor(600851475143L));
		
		System.out.println("Largest palindrome product of two three-digit numbers is: " 
				+ projEu.findLargestPalindromeProduct(999));

		System.out.println("Smallest integer divisible by all numbers from 1 to 20 is: " 
				+ projEu.leastCommonMultiple(20));
		
		System.out.println("The sum square difference of the first 100 natural numbers is: " + 
				+ projEu.sumSquareDifference(100));
		
//		System.out.println("The 10 001st prime number is: " + 
//				+ projEu.nThPrime(10001));
		
		String num = "";
		System.out.println("Largest product of 5 consecutive numbers: " + projEu.largestProductOfFiveConsecutiveNumbers(num));
		
		System.out.println("Product of pytagorean triple whose sum = 1000: "+ projEu.pythagoreanTriple(1000));
		
		System.out.println("Sum of primes whose value does not exceed two million: " + projEu.sumOfPrimes(2000000));
		

		long endTime = System.nanoTime();
		long elapsed = endTime - startTime;
		double seconds = (double) elapsed / 1000000000.0;
		System.out.println("\nAll calculations took: " +seconds+ " seconds.");
		
		
		int[][] grid = new int[][] {{4, 4, 4, 4, 4}, {2, 2, 2, 2, 2}, {3, 3, 1, 1, 3}, {1, 9, 9, 9, 9}, {1, 1, 1, 1, 1}};
		
		for (int row = 0; row < grid.length; row++) { //let's start by going sideways
			for (int column = 0; column < grid[row].length-3; column++) {
				for (int i = 0; i < 4; i++){
					System.out.print( grid[row][column+i]+" ");	
				}
				System.out.print("\n");
			}
						
		}				
		

	}
	

}
